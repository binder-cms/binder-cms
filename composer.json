{
    "name": "binder-cms/binder-cms",
    "type": "symfony-bundle",
    "description": "The source-controlled CMS",
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Phillips",
            "email": "ian.f.phillips@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "^3.3",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Binder\\": "src/Binder/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Binder\\": "tests/Binder"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "dev-dev"
        }
    }
}
